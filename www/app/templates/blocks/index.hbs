{{#if model.matured}}
<h4>Matured Blocks</h4>
<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th style="min-width:100px;">Height</th>
        <th>Block Hash</th>
        <th style="min-width:175px;">Time Found</th>
        <th style="min-width:50px;">Variance</th>
        <th style="min-width:75px;">Reward</th>
        <th style="min-width:50px;">Type</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.matured as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>No matured blocks yet</h3>
{{/if}}
